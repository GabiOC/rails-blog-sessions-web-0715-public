<%= flash[:notice] %>
<%= form_tag(sessions_path, method: :post) do %>
<div class="field">
  <%= label_tag :email %><br>
  <%= email_field_tag :email%><br>
  <%= label_tag :password %><br>
  <%= password_field_tag :password %><br>
</div>
<div class="actions">
  <%= submit_tag "Log In" %>
</div>
<% end %>
